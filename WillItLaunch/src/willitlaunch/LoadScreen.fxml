<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0" styleClass="background" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="willitlaunch.LoadScreenController">
    <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label alignment="CENTER" prefWidth="3000.0" styleClass="header" text="Will It Launch?!" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font size="72.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="100.0" left="100.0" right="100.0" top="100.0" />
               </VBox.margin>
            </Label>
            <StackPane fx:id="logoPane" prefHeight="150.0" prefWidth="200.0" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0" VBox.vgrow="ALWAYS">
               <children>
                  <TextField fx:id="ipBox" prefWidth="400.0" promptText="Connect to Mission Control" text="ws://10.0.1.23:8080/ws" />
                  <Button fx:id="connect" mnemonicParsing="false" onAction="#connectTo" text="Connect" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
    </children>
</AnchorPane>
